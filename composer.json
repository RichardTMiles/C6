{
  "name": "richardtmiles/c6",
  "description": "The best MVC framework",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "config": {
    "vendor-dir": "vendor"
  },

  "autoload": {
    "psr-4": {
      "App\\": "",
      "View\\": "view/",
      "Controller\\": "Controller/",
      "Model\\": "model/",
      "Table\\": "table/"
    }
  },
  "scripts": {
    "pre-install-cmd":"composer global require fxp/composer-asset-plugin:dev-master",
    "pre-update-cmd":"composer global require fxp/composer-asset-plugin:dev-master"
  },
  "authors": [
    {
      "name": "Richard Miles",
      "email": "RichardMiles@Stats.Coach"
    }
  ],

  "require-dev": {
    "richardtmiles/carbonphp" : "dev-master",
    "google/cloud-tools": "^0.6"
  },

  "require": {
    "php": ">=7.1",
    "google/cloud": ">=0.21",
    "facebook/graph-sdk": "^5.6.1",
    "google/apiclient": "dev-master",
    "mustache/mustache": "dev-master",
    "almasaeed2010/adminlte":"v2.4.0",
    "bower-asset/jquery-backstretch": "dev-master",
    "bower-asset/jquery-form": "dev-master",
    "bower-asset/jquery-pjax": "dev-master",
    "bower-asset/mustache.js": "dev-master"
  }
}
